    "value =  input(\"请输入一个数值:\")\n",
    "\n",
    "if value.isdigit():\n",
    "    number = int(value)\n",
    "    if 0<=number<=100:\n",
    "        if number<60:\n",
    "            print(\"不及格\")\n",
    "        else:\n",
    "            print(\"及格\")\n",
    "    else:\n",
    "        print(\"数值不在0～100之间\")\n",
    "else:\n",
    "    print(\"输入的不是一个数值\")"
